<div class="card review-card">
    <div class="card-header">
        {{review_title}}
    </div>
    <div class="card-body">
        <blockquote class="blockquote mb-0">
        <p>Rating: {{rating}}</p>
        <p>{{review_text}}</p>
        <footer class="blockquote-footer">{{username}}</footer>
        </blockquote>
    </div>
</div>